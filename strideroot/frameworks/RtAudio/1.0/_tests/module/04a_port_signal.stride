use DesktopAudio version 1.0

import Generators


module Test {
	ports: [
		port OutputPort {
			main: on
			direction: "output"
			block: Output
		}
		port InputPort {
			main: on
			direction: "input"
			block: Input
		}
		port TestPort {
			name: "test"
			direction: "input"
			block: TestBlock
		}
	]
	streams: [
		Input * TestBlock >> Output;
		]
}

signal Half {default: 0.5}
signal Quarter {default: 0.25}

AudioIn[1] >> Test(test: Half) >> AudioOut[1];
AudioIn[2] >> Test(test: Quarter) >> AudioOut[2];
