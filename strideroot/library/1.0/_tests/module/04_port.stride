use DesktopAudio version 1.0

import Generators


module Test {
	ports: [
		port OutputPort {
			main: on
			direction: "output"
			block: Output
		}
		port InputPort {
			main: on
			direction: "input"
			block: Input
		}
		port TestPort {
			name: "test"
			direction: "input"
			block: TestBlock
		}
	]
	streams: [
		Input * TestBlock >> Output;
#		[Input, TestBlock] >> Multiply() >> Output;
#		Input >> Multiplication( value: TestBlock) >> >> Output;
		
		]
}

AudioIn[1] >> Test(test: AudioIn[2]) >> AudioOut[1];
