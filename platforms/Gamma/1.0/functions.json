{"functions": [
        {"functionName": "Level",
            "ports" : {
                "gainType" : {"in_types" : ["CSS", "CS"], "out_types" : [], "default" : "linear"},
                "gain" : {"in_types" : ["AS", "CSR", "CSI", "CR", "CI"], "out_types" : [], "default" : 1.0},
                "bias" : {"in_types" : ["AS", "CSR", "CSI", "CR", "CI"], "out_types" : [], "default" : 0.0},
                "bypass" : {"in_types" : ["CSB", "CB"], "out_types" : [], "default": false }
            },
            "num_inputs" : 1,
            "num_outputs" : 1,
            "in_types" : ["AS", "CSR"],
            "out_types" : [ "AS", "CSR"],
            "code" : {
                "includes" : {"code": ""},
                "init_code": {"code": ""},
                "dsp_code": {"code": "if (!%%property:bypass%%) %%token%% = %%property:bias%% + %%token%%  * %%property:gain%% ;\n"}
            }
        },
        {
            "functionName": "LowPass",
            "ports": {
                "cutOff" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": 1000},
                "reset" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": {"type": "none"}},
                "bypass" : {"in_types": ["CBP"], "out_types" : [], "default": false}
            },
            "num_inputs" : 1,
            "num_outputs" : 1,
            "in_types" : ["AS", "CSR"],
            "out_types" : [ "AS", "CSR"],
            "code" : {
                "includes": {"code": "Gamma/Filter.h"},
                "init_code": {"code": "Biquad<> %%identifier%%(%%property:cutOff%%, 1, LOW_PASS);\n"},
                "dsp_code": {"code": "if (!%%property:bypass%%) %%token%% = %%identifier%%(%%token%%);\n"}
            }
        },
        {
            "functionName": "Oscillator",
            "ports": {
                "type" : {"in_types": ["CSP", "CS"], "out_types" : [], "default": "sine"},
                "amplitude" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": 1.0},
                "frequency" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": 440.0},
                "phase" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": 0.0},
                "reset" : {"in_types": ["CBP"], "out_types" : [], "default": false}
            },
            "num_inputs" : 1,
            "num_outputs" : 1,
            "in_types" : [],
            "out_types" : [ "AS", "CSR"],
            "code" : {
                "includes": {"code": "Gamma/Oscillator.h"},
                "init_code": {"code": "Sine<> %%identifier%%(%%property:frequency%%, %%property:phase%%);\n"},
                "dsp_code": {
                    "code": "%%token%% = %%identifier%%();\n",
                    "frequency": "%%identifier%%.freq(%%intoken%%);\n"
                }
            }
        },
        {
            "functionName": "Map",
            "ports": {
                "input": {"in_types" : ["ASP", "CSIP", "CSRP"], "out_types" : [], "default": {"type": "none"}},
                "output": {"in_types" : ["CSIP", "CSRP"], "out_types" : [], "default": {"type": "none"}, "required" : false, "maxconnections" : -1, "access": "stream_out"},
                "default": {"in_types" : ["CIP", "CRP"], "out_types" : [], "default": 0.0, "required" : false, "maxconnections" : 1, "access": "property"},
                "rate": {"in_types" : ["CIP", "CRP"], "out_types" : [], "default": "ControlRate", "required" : false, "maxconnections" : 1, "access": "property"},
                "enableMapping": {"in_types" : ["CBP"], "out_types" : [], "default": "on", "required" : false, "maxconnections" : 1, "access": "property"},
                "mappingMode": {"in_types" : ["CSP"], "out_types" : [], "default": "'linear'", "required" : false, "maxconnections" : 1, "access": "property"},
                "minimum": {"in_types" : ["CIP", "CRP"], "out_types" : [], "default": 0.0, "required" : false, "maxconnections" : 1, "access": "property"},
                "maximum": {"in_types" : ["CIP", "CRP"], "out_types" : [], "default": 1.0, "required" : false, "maxconnections" : 1, "access": "property"},
                "increment": {"in_types" : ["CIP", "CRP", "none"], "out_types" : [], "default": {"type": "none"}, "required" : false, "maxconnections" : 1, "access": "property"},
                "rounding": {"in_types" : ["CSP"], "out_types" : [], "default": "'truncate'", "required" : false, "maxconnections" : 1, "access": "property"}
            },
            "num_inputs" : 1,
            "num_outputs" : 1,
            "in_types" : [],
            "out_types" : [ "AS", "CSR"],
            "code" : {
                "includes": {"code": ""},
                "init_code": {"code": ""},
                "dsp_code": {
                    "code": "%%token%% = %%property:minimum%% + (%%intoken%% + 1.0) * 0.5 * (%%property:maximum%% - %%property:minimum%%);\n"
                }
            }
        },


        {
            "functionName": "Pan",
            "ports": {
                "position" : {"in_types": ["CIP", "CFP"], "out_types" : [], "default": 0.0},
                "bypass" : {"in_types": ["CBP"], "out_types" : [], "default": false}
            },
            "num_inputs" : 1,
            "num_outputs" : 2,
            "in_types" : [],
            "out_types" : [ "AS", "CSR"],
            "code" : {
                "includes": {"code": "Gamma/Effects.h"},
                "init_code": {"code": "Pan<> %%identifier%%(%%property:position%%);\n"},
                "dsp_code": {
                    "code": "%%identifier%%(%%intoken%%,%%token:1%%, %%token:2%%);\n",
                    "position": "%%identifier%%.pos(%%intoken%%);\n"
                }
            }
        }
    ]
}



