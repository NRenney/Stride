use Gamma version 1.0 on PC

import Generators

signal Frequency { 
	default: 440
	domain: AudioDomain
}

reaction Test {
	streams: [
		Frequency + 50 >> Frequency;
	]
}

Impulse(frequency:1) >> Trig;

[ Trig, 1 ] >> Equal() >> Test();

Oscillator(frequency: Frequency) >> Level(gain:0.2) >> AudioOut;

