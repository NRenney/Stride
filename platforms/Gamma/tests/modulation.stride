use Gamma version 1.0 on PC

import generators

constant CarrierFreq { value: 440}
constant CMRatio { value: 3 }
constant ModFreq { value: CarrierFreq * CMRatio }
constant Index { value: 2 }

Oscillator (
        frequency:      ModFreq
) >> Modulator;

Oscillator (frequency: CarrierFreq + Modulator * (CarrierFreq * Index)
	amplitude: 0.2) 
>> AudioOut;
