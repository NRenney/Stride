use Gamma version 1.0 on PC

module Test {
	ports: [
		port OutputPort {
			main: on
			direction: "output"
			block: Output
			domain: OutputDomain
		}
		port InputPort {
			main: on
			direction: "input"
			block: Input
		}
		port TestPort {
			name: "test"
			direction: "input"
			block: TestBlock
			domain: OutputDomain
		}
		port TestPort2 {
			name: "test2"
			direction: "input"
			block: TestBlock2
			domain: OutputDomain
		}
	]
	blocks: [
#		signal Amp { domain: TestDomain }
	]
	streams: [
		TestBlock * TestBlock2 >> Amp;
		Input * Amp >> Output;
	]
}

module Test2 {
	ports: [
		port TestPort2 {
			name: "test2"
			direction: "input"
			block: TestBlock2
			domain: OutputDomain
		}
		port InputPort {
			main: on
			direction: "input"
			block: Input
		}
		port TestPort {
			name: "test"
			direction: "input"
			block: TestBlock
			domain: OutputDomain
		}
		port OutputPort {
			main: on
			direction: "output"
			block: Output
			domain: OutputDomain
		}
	]
	blocks: [
#		signal Amp { domain: TestDomain }
	]
	streams: [
		TestBlock * TestBlock2 >> Amp;
		Input * Amp >> Output;
	]
}

signal SigIn[2] { domain: AudioDomain}

AudioIn >> SigIn;

SigIn[1] >> Test(test: AudioIn[1] test2:AudioIn[2]) >> AudioOut[1];

SigIn[2] >> Test2(test: AudioIn[1] test2:AudioIn[2]) >> AudioOut[1];
