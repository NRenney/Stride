use Gamma version 1.0 on PC

import generators


module Test {
	ports: [
		port OutputPort {
			main: on
			direction: "output"
			block: Output
		}
		port InputPort {
			main: on
			direction: "input"
			block: Input
		}
		port TestPort {
			name: "test"
			direction: "input"
			block: TestBlock
		}
	]
	streams: [
		
		Input * TestBlock >> Output;
	]
}

AudioIn[1] >> Test(test: AudioIn[2]) >> AudioOut[1];
