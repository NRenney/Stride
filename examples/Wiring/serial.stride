use Wiring version 1.0

import Generators
import Serial

signal DownSample {
    rate:   5
    domain: PlatformDomain
}

signal Frequency {
    domain: PlatformDomain
}

1 + AnalogIn[1] * (1.0/128.0) >> Frequency;

AnalogIn[2] + AnalogIn[3] >> DownSample >> SerialPrint();

[ Oscillator ( frequency: Frequency ) , 0 ] >> Greater() >> DigitalOut[14];
