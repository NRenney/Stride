
use DesktopAudio version 1.0

AudioIn[1] >> Signal >> AudioOut[1];


signal Rate1 {
    rate: 22050
}


signal Rate2 {
    rate: 11025
}


Rate1 >> Rate2 >> Output1;

Signal1 >> Rate1 >> Signal2 >> Rate2 >> GetAudioRate >> Output2;

TestModule() >> Rate1 >> Level(gain: 0.5) >> Output3;

module TestModule {
    ports: [
        port OutputPort {
            main: on
            direction: "output"
            block: Output
        }
    ]
    streams: [
        Test >> Output;
    ]
}

