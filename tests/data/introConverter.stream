	use PufferFish version 1.1
	
	switch Gate {
		default:	off
		meta:		'Gate is ON when the value of analog input 1 is above 5.0.'
	}
	
	switch InvertedGate {
		default:	on
		meta:		' Inverted value of Gate.'
	}
	
	AnalogIn_1 
	>> control2switch ( 
		threshold:	5.0
		invert:		off
	) 
	>> Gate
	>> invertSwitch()
	>> InvertedGate;
	
	
	AudioIn[1]
	>> gainDB (
			levelDB:        -96.0
			bypass:         Gate
	)
	>> AudioOut[1];
	
	AudioIn[1]
	>> gainDB (
			levelDB:        -96.0
			bypass:         InvertedGate
	)
	>> AudioOut[2];